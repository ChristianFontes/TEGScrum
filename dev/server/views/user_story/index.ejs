<table align="center" class="mdl-data-table mdl-js-data-table mdl-shadow--2dp">
  <thead>
    <tr>
      <th class="mdl-data-table__cell--non-numeric">ID</th>
      <th>Nombre</th>
      <th>Descripcion</th>
      <th>Estado</th>
      <th>Mostrar</th>
      <th>Eliminar</th>
    </tr>
  </thead>
  <tbody>
  <% _.each(users_stories, function(user_story){ %>
    <tr>
      <td class="mdl-data-table__cell--non-numeric"><%= user_story.id %></td>
      <td><%= user_story.nombre %></td>
      <td><%= user_story.descripcion %></td>
      <td><%= user_story.estado %></td>
      <td><a href="/user_story/show/<%= user_story.id %>" class="mdl-button mdl-js-button mdl-button--primary">Mostrar</a></td>
      <td>
        <form action="/user_story/destroy/<%= user_story.id %>" method="POST">
          <input type="hidden" name="_method" value="delete">
          <input type="submit" class="mdl-button mdl-js-button mdl-button--primary" value="Eliminar">
        </form>
      </td>
    </tr>
  <% }) %>
  </tbody>
</table>