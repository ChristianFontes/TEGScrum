<div class="mdl-layout mdl-js-layout mdl-color--grey-100" align="center">
    <main class="mdl-layout__content">
        <div class="mdl-card mdl-shadow--6dp">
            <div class="mdl-card__title mdl-color--primary mdl-color-text--white">
                <h2 class="mdl-card__title-text">Editar Perfil</h2>
            </div>
            <div class="mdl-card__supporting-text">
                <form action="/project/update/<%= project.id %>" method="POST" class="form-signin">
                        
                    <div class="mdl-textfield mdl-js-textfield">
                        <input class="mdl-textfield__input" class="form-control" type="text" name="name" value="<%= project.name %>">
                        <label class="mdl-textfield__label" class="form-control">Nombre</label>
                    </div>
                    <div class="mdl-textfield mdl-js-textfield">
                        <input class="mdl-textfield__input" class="form-control" type="text" name="row2" value="<%= project.row2 %>">
                        <label class="mdl-textfield__label" class="form-control">Campo (opcional)</label>
                    </div>
                    <div class="mdl-textfield mdl-js-textfield">
                        <input class="mdl-textfield__input" class="form-control" type="text" name="row3" value="<%= project.row3 %>">
                        <label class="mdl-textfield__label" class="form-control">Campo (opcional)</label>
                    </div>
                        <div class="mdl-card__actions mdl-card--border">
                            <button type="submit" class="mdl-button mdl-button--colored mdl-js-button mdl-js-ripple-effect">Editar</button>
                    </div>
                </form>
            </div>
        </div>
    </main>
</div>